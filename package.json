  "version": "0.2.1",
  "packageManager": "pnpm@10.18.3",
  "sideEffects": false,
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "release": "pnpm run build && publint && changeset publish"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.7",
    "@types/react": "^18.3.26",
    "prettier": "^3.6.2",
    "publint": "^0.3.14",
    "react": "^18.3.1",
    "tsup": "^8.5.0",
    "typescript": "^5.9.3"
  },
  "author": {
    "email": "doinki94@gmail.com",
    "name": "doinki",
    "url": "https://github.com/doinki"
  },
  "bugs": "https://github.com/doinki/modoru/issues",
  "homepage": "https://github.com/doinki/modoru#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/doinki/modoru.git"
  },
  "publishConfig": {
    "access": "public"
  }
