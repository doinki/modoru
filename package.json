{
  "name": "modoru",
  "license": "MIT",
  "version": "0.0.3",
  "packageManager": "pnpm@9.12.2",
  "sideEffects": false,
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "lint": "eslint --cache --ignore-path .gitignore .",
    "typecheck": "tsc --noEmit",
    "release": "pnpm run build && publint && changeset publish"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.9",
    "@types/react": "^18.3.11",
    "eslint": "^8.57.1",
    "eslint-config-mado": "^0.4.2",
    "prettier": "^3.3.3",
    "publint": "^0.2.11",
    "react": "^18.3.1",
    "tsup": "^8.3.0",
    "typescript": "^5.6.3"
  },
  "pnpm": {
    "overrides": {
      "esbuild": "^0.24.0"
    }
  },
  "author": {
    "email": "doinki94@gmail.com",
    "name": "doinki",
    "url": "https://github.com/doinki"
  },
  "bugs": "https://github.com/doinki/modoru/issues",
  "homepage": "https://github.com/doinki/modoru#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/doinki/modoru.git"
  },
  "publishConfig": {
    "access": "public"
  }
}
